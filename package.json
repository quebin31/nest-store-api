{
  "name": "nest-store-api",
  "version": "0.1.0",
  "description": "Store API",
  "author": "Kevin Del Castillo",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "npm run redoc:build && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "redoc:watch": "redocly preview-docs api/openapi.yaml",
    "redoc:build": "redocly build-docs api/openapi.yaml -o build/redoc.html",
    "redoc:bundle": "redocly bundle api/openapi.yaml -o build/openapi.yaml",
    "setup:dev": "./scripts/setup-env.sh dev",
    "setup:test": "./scripts/setup-env.sh test",
    "migrate:dev": "dotenv -e .env.dev prisma migrate dev",
    "migrate:test": "dotenv -e .env.test prisma migrate deploy",
    "migrate:reset:dev": "dotenv -e .env.dev prisma migrate reset",
    "migrate:reset:test": "dotenv -e .env.test prisma migrate reset",
    "migrate:prod": "dotenv -e .env.prod prisma migrate deploy",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm run setup:test && jest --config test/jest.e2e.config.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@prisma/client": "^4.12.0",
    "@sendgrid/mail": "^7.7.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "nanoid": "^3.3.4",
    "nanoid-dictionary": "^4.3.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "redis": "^4.6.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@redocly/cli": "^1.0.0-beta.124",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.0",
    "@types/lodash.omit": "^4.5.7",
    "@types/lodash.pick": "^4.4.7",
    "@types/multer": "^1.4.7",
    "@types/nanoid-dictionary": "^4.2.0",
    "@types/node": "18.15.3",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "prisma": "^4.12.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.2",
    "typescript": "^4.9.5"
  }
}
